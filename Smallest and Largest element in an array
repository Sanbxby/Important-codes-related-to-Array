a = array = list(map(int, input("Enter the elements of the array separated by space: ").split()))
max_element = a[0]
min_element = a[0]

for i in range(len(a)):
  if a[i] > max_element:
     max_element = a[i]
  if a[i] < min_element:
      min_element = a[i]
  

print (max_element)
print (min_element)

def twosum(a,k):
    for i in range(len(a)):
        for j in range( i+1, len(a)):
            if  a[i]+a[j]==k:
                return i,j
    return -1
    
a = list(map(int, input("Enter the elements of the array separated by space: ").split()))
k=int(input("Enter target : "))
print (twosum(a,k))
        
